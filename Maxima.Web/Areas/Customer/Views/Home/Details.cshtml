@model ShoppingCart

<form method="post">
    <input hidden asp-for="ProductId" />

    <div class="row m-4">
        <div class="col-3">
            <img onclick="change_image(this)" src="@Model.Product.ImageUrl" class="w-50 ms-2">
            @foreach (var item1 in Model.Product.AdditionalImg)
            {
                <img onclick="change_image(this)" src="@item1.AdditionalImgUrl" class="w-50 m-2" />
            }
        </div>

        <div class="col-6 ">
            <img id="main-image" src="@Model.Product.ImageUrl" class=" w-100" />
        </div>

        <div class="col-3 ">
            <div class="product_card_accent_text text-uppercase m-2">@Model.Product.Category.Name</div>
            <div class="product_card_secondary_text text-uppercase m-2">@Model.Product.Name</div>
            <div class="product_card_secondary_text text-uppercase m-2">@Model.Product.Price €</div>

            <div class="sizes m-2  pt-4">
                <h6 class="text-uppercase product_card_main_text mb-0">Size</h6>
                @foreach (var item in Model.Product.Size)
                {
                    <label class="radio mt-2">
                        <input asp-for="Size" type="radio" name="size" value="@item.DisplayName"> <span>@item.DisplayName</span>
                    </label>
                }
            </div>

            <div class="m-2">
                <p class="text-uppercase product_card_main_text">Count</p>
                <div class="col-3"><input asp-for="Count" class="form-control"></div>
            </div>

            <div class="m-2">
                <button type="submit" value="Add to Cart" class="btn btn-continue form-control">ADD TO CART</button>
            </div>

            <div class="container p-0 m-2">
                <div class="row">
                    <div class="col-12 col-md-8">
                        <!-- Button trigger modal -->
                            <button type="button" class="btn btn-filter p-0 " style="font-size:small;" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
                                DELIVERY AND PAYMENT
                            </button>

                        <!-- Modal -->
                            <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header bg-gradient-secondary border-0">
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                        </div>
                                        <div class="modal-body bg-gradient-secondary">
                                            <h5 class="product_card_main_text text-md-center">   Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet.     </h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                     </div>

                    <div class="col-12 col-md-4">
                    
                            <!-- Button trigger modal -->
                            <button type="button" class="btn btn-filter p-0  " style="font-size:small;" data-bs-toggle="modal" data-bs-target="#staticBackdrop1">
                                RETURNS
                            </button>
                            <!-- Modal -->
                            <div class="modal fade" id="staticBackdrop1" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel1" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header bg-gradient-secondary border-0">
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                        </div>

                                        <div class="modal-body bg-gradient-secondary">
                                            <h5 class="product_card_main_text text-md-center">   dolor sit amet.     </h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                  </div>
             </div>
            </div>

            <div class=" pt-4">
                <!-- Button trigger modal -->
                <button type="button" class="btn btn-filter p-0" data-bs-toggle="modal" data-bs-target="#staticBackdrop3">
                    <svg width="80" height="60" viewBox="0 0 80 60" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="80" height="60" fill="url(#pattern0)" />
                        <defs>
                            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_17_300" transform="scale(0.000976562 0.00130208)" />
                            </pattern>
                            <image id="image0_17_300" width="1024" height="768" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg> SIZE GRID
                </button>
            </div>

            <!-- Modal -->
            <div class="modal fade" id="staticBackdrop3" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel3" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header bg-gradient-secondary border-0">
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body bg-gradient-secondary">
                            <img src="https://colourwear.com/media/uchplizn/women_size_guide.png" width="100%" class="rounded" />
                        </div>
                    </div>
                </div>
            </div>

            <div class="accordion accordion-flush mt-3" id="accordionFlushExample">
                <div class="accordion-item bg-transparent border-0">
                    <h2 class="accordion-header" id="flush-headingOne">
                        <button class="btn-filter collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                            DESCRIPRION <i class="bi bi-caret-down-fill"></i>
                        </button>
                    </h2>

                    <div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
                        <div class="accordion-body product_card_secondary_text text-md-center">@Model.Product.Description</div>
                    </div>
                </div>

                <div class="accordion-item bg-transparent border-0">
                    <h2 class="accordion-header" id="flush-headingTwo">
                        <button class="btn-filter collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
                            MATERIALS <i class="bi bi-caret-down-fill"></i>
                        </button>
                    </h2>

                    <div id="flush-collapseTwo" class="accordion-collapse collapse" aria-labelledby="flush-headingTwo" data-bs-parent="#accordionFlushExample">
                        <div class="accordion-body product_card_secondary_text text-md-center"> @Model.Product.Description</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>


